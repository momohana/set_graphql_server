# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Company {
  comid: String!
  companygroup: String!
  companyname: String!
  comcode: String!
}

type Query {
  companys: [Company!]!
}

input NewCompany {
  comid: String!
  companygroup: String!
  companyname: String!
  comcode: String!
}

type Mutation {
  createCompany(input: NewCompany!): Company!
}
